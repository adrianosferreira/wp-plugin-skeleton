{
  "name": "adrianoferreira/wp-plugin-skeleton",
  "description": "A simple skeleton for building WordPress plugins",
  "authors": [
    {
      "name": "Adriano Ferreira",
      "email": "adrianokta@gmail.com"
    }
  ],
  "require": {
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "10up/wp_mock": "^0.4.2",
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "3.*",
    "phpstan/phpstan": "^0.12.31",
    "szepeviktor/phpstan-wordpress": "^0.6.1"
  },
  "autoload": {
    "psr-4": {
      "PluginMainNamespace\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PluginMainNamespace\\Tests\\": "tests/"
    }
  }
}
